{
  "extends": ["prettier"],
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
  },
  "rules": {
    "linebreak-style": 0,
    "no-underscore-dangle": 0,
    "import/prefer-default-export": 0,
  }
}
